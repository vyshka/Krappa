@model IEnumerable<WebApplication1.Models.ApplicationUser>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Scripts
{
    <script src="@Url.Content("~/Scripts/Custom.js")"></script>
}
<head>
    <title>Сптсок пользователей</title>
</head>
<body>
    <table class="table table-bordered table-condensed">
        <thead>
            <tr>
                <th style="text-align: center;">Имя</th>
                <th style="text-align: center;">Email</th>
                <th style="text-align: center;">Дата регистрации</th>
                <th style="text-align: center;">Редактировать</th>
                <th style="text-align: center;">Удалить</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    
                    <td style="text-align:center">@Html.DisplayFor(modelItem => item.UserName)</td>
                    <td style="text-align:center">@Html.DisplayFor(modelItem => item.Email)</td>
                    <td style="text-align:center">@Html.DisplayFor(modelItem => item.registerTime)</td>
                    <td style="text-align:center"><a href="@Url.Action("Edit", "Account")/@Html.DisplayFor(modelItem => item.Id)">Редактировать</td>
                    <td style="text-align:center"><a class="pointer-arrow" onclick="deleteRow(@Html.DisplayFor(modelItem => item.Id))">Удалить</td>
                </tr>
            }

        </tbody>
    </table>
</body>


